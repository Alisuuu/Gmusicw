{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/search",
      "dest": "/api/index.js"
    },
    {
      "src": "/play",
      "dest": "/api/index.js"
    },
    {
      "src": "/playlist-content",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}