{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/search",
      "dest": "/api/index.js"
    },
    {
      "src": "/play",
      "dest": "/api/index.js"
    },
    {
      "src": "/playlist-content",
      "dest": "/api/index.js"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/p2.png",
      "dest": "/p2.png"
    },
    {
      "src": "/sw.js",
      "dest": "/sw.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
